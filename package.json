{
  "name": "eslint-config-oclif-typescript",
  "description": "eslint config for Typscript'd oclif",
  "version": "0.2.0",
  "author": "oclif",
  "bugs": "https://github.com/oclif/eslint-config-oclif-typescript/issues",
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^2.6.1",
    "@typescript-eslint/parser": "^2.6.1",
    "eslint-config-xo-space": "^0.20.0",
    "eslint-plugin-mocha": "^5.2.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-unicorn": "^6.0.1"
  },
  "devDependencies": {
    "eslint": "^5.5.0",
    "typescript": "3.8.3"
  },
  "engines": {
    "node": ">= 10.0.0"
  },
  "files": [
    "/index.js"
  ],
  "homepage": "https://github.com/oclif/eslint-config-oclif-typescript",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "index.js",
  "repository": "oclif/eslint-config-oclif-typescript",
  "scripts": {
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "build": "rm -rf lib && tsc",
    "pretest": "yarn build --noEmit",
    "posttest": "yarn lint"
  }
}
